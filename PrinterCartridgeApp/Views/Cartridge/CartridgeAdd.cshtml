@model PrinterCartridgeApp.Models.Cartridge
@{
    ViewData["Title"] = "CartridgeAdd";
}

<h1>Cartridge Add</h1>

<h4>Cartridge</h4>
<hr/>
<div class="row">
    <div class="col=md-4">
        <form asp-action="CartridgeAdd">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Cartridge_Number" class="control-label"></label>
                <input asp-for="Cartridge_Number" class="form-control" />
                <span asp-validation-for="Cartridge_Number" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Cartridge_Type" class="control-label"></label>
                <input asp-for="Cartridge_Type" class="form-control" />
                <span asp-validation-for="Cartridge_Type" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Printer_Recd_For" class="control-label"></label>
                <input asp-for="Printer_Recd_For" class="form-control" />
                <span asp-validation-for="Printer_Recd_For" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Printer_Model" class="control-label"></label>
                <input asp-for="Printer_Model" class="form-control" />
                <span asp-validation-for="Printer_Model" class="text-danger"></span>
            </div>
            @*<div class="form-group">
                <label asp-for="Date_Keyed" class="control-label"></label>
                <input asp-for="Date_Keyed" class="form-control" />
                <span asp-validation-for="Date_Keyed" class="text-danger"></span>
            </div>*@
            <div class="form-group">
                <input type="submit" value="Add Cartridge" class="btn btn-primary"/>
            </div>
        </form>
    </div>
</div>
